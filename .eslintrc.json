{
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    ]
  },
  "extends": ["next/core-web-vitals", "prettier", "eslint:recommended",
    "plugin:react/recommended", "plugin:react-hooks/recommended"]
}
